{
  "name": "react-todo-app",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "generate:svg": "svgo ./src/assets/icons/*.svg && gulp",
    "serve": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@commitlint/cli": "^13.1.0",
    "gulp": "^4.0.2",
    "gulp-plumber": "^1.2.1",
    "gulp-rename": "^2.0.0",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-svgo": "^2.2.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "svgo": "^2.5.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^3.1.5",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "autoprefixer": "^10.3.4",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.25.1",
    "husky": "^7.0.0",
    "lint-staged": ">=10",
    "postcss": "^8.3.6",
    "prettier": "^2.3.2",
    "tailwindcss": "^2.2.9",
    "typescript": "^4.3.2",
    "vite": "^2.5.2"
  },
  "lint-staged": {
    "*.{css,js,ts,tsx}": "prettier --write",
